#include <stdio.h>

int main(int argc, char** argv){
  int h = 1024;
  int v = 1024;
  int n = h*v;
  FILE *fp;
  fp = fopen("/home/gpu/vgopalas/sand2/src.txt","w");
  fprintf(fp,"1024 1024");
  for(int i = 0;i<n;i++){
    if(i%h ==0){
      fprintf(fp,"\n");
    }
    if(i>(h*(499)+500)&&i<(h*519+520)&&(i%h > 500)&&(i%h<520)){
      fprintf(fp, "2000 ");
    }
    else{
      fprintf(fp, "0 ");
    }
  }
  fclose(fp);
  return(1);
}
